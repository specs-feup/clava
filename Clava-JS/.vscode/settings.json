{
  "prettier.tabWidth": 2,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "./api/**": true,
    "./code/**": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "./api/**": true,
    "./code/**": true,
    "./coverage/**": true,
    "./woven_code/**": true
  },
  "cmake.configureOnOpen": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
