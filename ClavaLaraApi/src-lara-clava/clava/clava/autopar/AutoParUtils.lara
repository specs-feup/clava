import clava.Clava;

/**
 * Utility methods related to the AutoPar package.
 *
 * @class
 */
var AutoParUtils = {};

/**
 * Returns the petit executable, of the Omega suite.
 *
 * @return {J#File} a file representing the petit executable.
 */
AutoParUtils.getPetit = function() {
	return Clava._getApiUtils().getPetitExecutable();
}

